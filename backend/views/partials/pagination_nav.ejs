<nav aria-label="...">
    <ul class="pagination justify-content-center">
      <li class="page-item <%= current ==1 ? "disabled" :"" %>">
        <a class="page-link" href="/characters?page=<%= current-1 %>" tabindex="-1">Previous</a>
      </li>
      <% if (page > 1) { %>
        <li class="page-item"><a class="page-link" href="/characters?page=<%= current-1 %>"><%=current-1%> </a></li>

        <%}%>

      <li class="page-item active">
        <a class="page-link" href="#"><%=current%> <span class="sr-only">(current)</span></a>
      </li>
      <li class="page-item"><a class="page-link" href="/characters?page=<%= current+1 %>"><%=current+1%> </a></li>
      <li class="page-item">
        <a class="page-link" href="/characters?page=<%= current+1 %>" >Next</a>
      </li>
    </ul>
  </nav>